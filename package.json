        "sass": "^1.63.6",
        "sass-loader": "^13.3.2",
        "vue-loader": "^17.2.2",
        "vue-template-compiler": "^2.7.14"
    },
    "dependencies": {
        "vue": "^3.3.4",
        "vue-froala-wysiwyg": "^4.1.0"
